  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  
  <filter> 
      <filter-name>LoginFilter</filter-name>
      <filter-class>it.polimi.tiw.progetto.filter.LoginFilter</filter-class> 
 </filter>
 
 <filter-mapping> 
    <filter-name>LoginFilter</filter-name> 
  	<url-pattern>/HomePage</url-pattern>
  	<url-pattern>/addFile</url-pattern>
  	<url-pattern>/HomePage/addFile</url-pattern>
  	<url-pattern>/HomePage/Sposta</url-pattern>
